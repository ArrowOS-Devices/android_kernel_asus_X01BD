&pm660l_gpios {
	gpio@c400 {
		status = "ok";
	};
};

&pm660l_wled {
	qcom,fs-current-limit = <20000>;
};

&pm660l_lpg {

    // Green
    lpg2 {
        qcom,ramp-step-ms = <40>;
    };

    // Red
    lpg3 {
        qcom,ramp-step-ms = <40>;
    };
};

&pm660l_flash0 {
	qcom,max-current = <1000>;
};

&pm660l_flash1 {
	qcom,max-current = <280>;
	qcom,current-ma = <80>;
};

&pm660l_torch1 {
	qcom,max-current = <80>;
	qcom,current-ma = <80>;
};

&pm660l_switch0 {
	qcom,led-mask = <1>;
};

&pm660l_switch1 {
	qcom,led-mask = <2>;
};

&pm660a_labibb {
	status = "okay";
};

&ibb_regulator {
	qcom,qpnp-ibb-limit-maximum-current = <200>;
	qcom,qpnp-ibb-limit-max-current-enable;
};

&lab_regulator {
	qcom,qpnp-lab-limit-maximum-current = <200>;
	qcom,qpnp-lab-limit-max-current-enable;
};
